<div class="container mt-5 text-center">
  <h1>Welcome to No_Airbnb</h1>
<div class="index-grid">
<% @spaceflights.each do |spaceflight| %>
<%= link_to spaceflight_path(spaceflight) do %>
  <div class="card" style="width: 18rem;">
    <%# <img class="card-img-top" src="<%=image_path('Serenityship.jpg')%>" alt="Card image cap"> %>
    <div class="card-body">
      <p class="card-text">
      <%= spaceflight.overview %>
              <%= spaceflight.destination %>
              <%= spaceflight.cabin_class %>
    </p>
    </div>
  </div>
  <% end %>
<% end %>
</div>
</div>
